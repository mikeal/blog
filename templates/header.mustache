<html>
  <head>
    <title>{{title}}</title>
    <meta name="description" content="{{description}}" />
    <meta name="keywords" content="{{meta}}" />
    <meta name="author" content="{{author}}" />
    <script language="javascript" type="text/javascript" src="/static/jquery.js" ></script>
    <script language="javascript" type="text/javascript" src="/static/prettydate.js" ></script>
    
    <script type="text/javascript" src="/static/syntaxhighlighter/shCore.js"></script>
  	<script type="text/javascript" src="/static/syntaxhighlighter/shBrushCss.js"></script>
  	<script type="text/javascript" src="/static/syntaxhighlighter/shBrushDiff.js"></script>
  	<script type="text/javascript" src="/static/syntaxhighlighter/shBrushJScript.js"></script>
  	<script type="text/javascript" src="/static/syntaxhighlighter/shBrushPlain.js"></script>
  	<script type="text/javascript" src="/static/syntaxhighlighter/shBrushPython.js"></script>
  	<link type="text/css" rel="stylesheet" href="/static/syntaxhighlighter/shCore.css"/>
  	<link type="text/css" rel="stylesheet" href="/static/syntaxhighlighter/shThemeDefault.css"/>
  	
    <link href="/static/site.css" rel="stylesheet" type="text/css" />
  <head>
  <body>
    <div id="title-container" class="title-container">
      <div id="title" class="title">{{title}}</div>
      <div id="author" class="author">{{author}}</div>
    </div>
    <script type="text/javascript">
      $(document).ready(function () {    		
        SyntaxHighlighter.config.clipboardSwf = '/static/syntaxhighlighter/clipboard.swf';
        SyntaxHighlighter.defaults['wrap-lines'] = false;
        SyntaxHighlighter.defaults.light = true;
        SyntaxHighlighter.defaults['tab-size'] = 2;
    		SyntaxHighlighter.all();
        $('div.blog-post-dt').prettyDate();
      })
    </script>
    <div id="body-container" class="body-container">